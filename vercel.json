{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/next",
      "config": {
        "distDir": ".next"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "frontend/$1"
    }
  ],
  "env": {
    "BACKEND_URL": "https://your-backend-url.com"
  },
  "github": {
    "silent": true
  },
  "outputDirectory": "frontend"
}